int main() {
    int i; int n;
    cout<<"enter the value of n";
    cin>>n;
        int ar[n];
    for(i=0;i<n;i++)
    {
        cout<<" enter the element of array"<< i + 1 << ": ";
     cin>>ar[i];
    }
   cout << "\nYou entered: ";
    for (int i = 0; i < n; i++) {
        cout << ar[i] << " ";
    }
    cout << endl;

    
    

    return 0;
}
