// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
   int n;int i; int s;
   cout<<"Enter the value of n";
   cin>>n;
   int ar[n];
  for(i=0;i<n;i++)
   {   cout<<"enter the element of array"<<i+1;
       cin>>ar[i];
   }
   cout<<"enter the searching number";
   cin>>s;
   if(s==ar[i])
   cout<<" found";
   else
   cout<<"not found";
    return 0;
}

OUTPUT:
Enter the value of n5
enter the element of array14
enter the element of array24
enter the element of array33
enter the element of array45
enter the element of array56
enter the searching number8
not found
