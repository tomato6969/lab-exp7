#include <iostream>
using namespace std;
int main() {
   int n;int i;
   cout<<"Enter the value of n ";
   cin>>n;
   int ar[n];
  for(i=0;i<n;i++)
   {   cout<<"enter the element of array " << i+1 ;
       cin>>ar[i];
       int max=ar[0];int min=ar[0];
   if(ar[i]>=max)
    max=ar[i];
   cout<< max;
    if (ar[i]<=min)
   min=ar[i];
   cout<<endl;
   cout<< min;
   }
   
       return 0;
}

OUTPUT:
